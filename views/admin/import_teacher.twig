{% extends 'layout.twig' %}

{% block content %}
    <h2>Importar Docentes</h2>
    <a href="{{ 'admin/teachers' | url }}">Atras</a>
    <div class="container-register">                              
        <form class="group-content-register" action="" method="post" enctype="multipart/form-data">
            <div class="wrap-register">
                <label for="title"><b>Lista de Docentes<strong class="check">*</strong></b></label>
                <input type="file" accept=".csv" name="listaDocentes" id="listaDocentes"><br>
                <input type="submit" value="ImportarDocentes">
                
            </div>
        </form>
    </div>
{% endblock %}